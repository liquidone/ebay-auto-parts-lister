# eBay Auto Parts Lister - Environment Variables
# Copy this file to .env and fill in your actual API keys

# Gemini AI API Key (Required for part identification)
# Get from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_actual_gemini_api_key_here

# OpenAI API Key (Optional fallback)
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_actual_openai_api_key_here

# eBay API Credentials (Required for listing)
# Get from: https://developer.ebay.com/my/keys
EBAY_APP_ID=your_actual_ebay_app_id
EBAY_DEV_ID=your_actual_ebay_dev_id
EBAY_CERT_ID=your_actual_ebay_cert_id
EBAY_USER_TOKEN=your_actual_ebay_user_token

# Feature Flags
ENABLE_BROWSER_FALLBACK=false
ENABLE_DEBUG_LOGGING=false

# Database Configuration
DATABASE_URL=sqlite:///./auto_parts.db

# Server Configuration
HOST=0.0.0.0
PORT=8000
