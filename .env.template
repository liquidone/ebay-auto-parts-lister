# eBay Auto Parts Lister Configuration
# Copy this file to .env and fill in your API keys

# Google Gemini API Key (primary AI service for part identification)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API Key (fallback AI service)
OPENAI_API_KEY=your_openai_api_key_here

# eBay API Configuration
# Get these from: https://developer.ebay.com/my/keys
EBAY_APP_ID=your_ebay_app_id_here
EBAY_DEV_ID=your_ebay_dev_id_here
EBAY_CERT_ID=your_ebay_cert_id_here
EBAY_USER_TOKEN=your_ebay_user_token_here
EBAY_SANDBOX=true

# eBay API Environment (sandbox or production)
EBAY_ENVIRONMENT=sandbox

# Optional: Additional API keys for enhanced functionality
# Note: GEMINI_API_KEY above is the primary service
# GOOGLE_VISION_API_KEY=your_google_vision_key_here
